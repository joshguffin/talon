Panel right: key(alt-ctrl-2)
panel left: key(alt-ctrl-1)

panel one:   key(ctrl-f1)
panel two:   key(ctrl-f2)
panel three: key(ctrl-f3)
panel four:  key(ctrl-f4)

window close: key(alt-f4)
window next:  key(ctrl-alt-tab)

switch:
    key(alt:down)
    key(tab)

switch to: key(alt:up)

jump: key(end)
hop: key(home)
